{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": ["NODE_ENV", "ACCOUNT_ID", "DATABASE_ID", "TOKEN"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "build:package": {
      "dependsOn": ["^build:package"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "check-exports": {},
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": ["node_modules/.cache/eslint/.eslintcache"]
    },
    "lint:package": {
      "dependsOn": ["^lint:package"]
    },
    "preview": {
      "dependsOn": ["^preview"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": ["node_modules/.cache/typescript/*.tsbuildinfo"]
    }
  }
}
